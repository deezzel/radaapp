package control.servicimplem;

import control.IShipmentService;
import java.util.List;
import javax.ejb.Stateless;
import javax.ejb.LocalBean;
import javax.persistence.EntityManager;
import javax.persistence.PersistenceContext;
import model.Shipment;

/**
 *
 * @author Julia
 */
@Stateless
@LocalBean
public class ShipmentService extends Generic<Shipment> implements IShipmentService{
    @PersistenceContext(unitName = "RADAAPP-ejbPU")
    private EntityManager em;

    public ShipmentService() {
        super(Shipment.class);
    }
    
    

    public void persist(Object object) {
        em.persist(object);
    }

    // Add business logic below. (Right-click in editor and choose
    // "Insert Code > Add Business Method")

    @Override
    protected EntityManager getEntityManager() {
           return em;
    }

    @Override
    public List<Shipment> getByOrderName() {
       // throw new UnsupportedOperationException("Not supported yet.");
        return (List<Shipment>)em.createNamedQuery("Shipment.findAllByOrderName").getResultList();
    }
    
}
